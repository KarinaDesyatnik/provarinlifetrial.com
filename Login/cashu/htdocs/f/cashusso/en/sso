<script>

        var ref = getCookie('CASHURefarrer');
        var lpRef = getCookie('lpRefarrer');
        var cash_refer = '';

        var myRefarrer = document.referrer.split( '/' );
        var myRefarrer = document.referrer;

        var results;
        if(lpRef && lpRef != ''){
            lpRef = decodeURIComponent(lpRef);
            results = lpRef.split('####') ;
            myRefarrer = results[0];
        }
 
        if(ref && ref != ''){
            ref = decodeURIComponent(ref);
            cash_results = ref.split('####') ;
            cash_refer = cash_results[0];

        }

        var convertedUrl = getHostName(myRefarrer);

        if(myRefarrer != '' && convertedUrl.search('cashu.') == -1 && convertedUrl.search('.cashu.') == -1 && myRefarrer != cash_refer){
            deleteCookie('CASHURefarrer');
            setCookie('CASHURefarrer', myRefarrer, 1);
        }

        function getHostName(url) {
            var match = url.match(/:\/\/(www[0-9]?\.)?(.[^/:]+)/i);
            if (match != null && match.length > 2 && typeof match[2] === 'string' && match[2].length > 0) {
            return match[2];
            }
            else {
                return null;
            }
        }

        function setCookie(cname, cvalue, exdays) {
            var d = new Date();
            d.setTime(d.getTime() + (exdays*24*60*60*1000*30));

            var expiresDate = d.toUTCString();
            var expires = 'expires='+ expiresDate;

            var expires2 = d.toJSON(); 
            expires2 = expires2.replace('T', ' '); 
            expires2 = expires2.replace('Z', ''); 
            var Fullexpires = expires2.split('.'); 
            expires2 = Fullexpires[0];

            var dom = window.location.hostname;

            cvalue = cvalue + '####' + expires2;

            document.cookie = cname +'=' + cvalue + ';expires=' + expires + ';domain=' + dom + ';path=/';
        }

        function getCookie(cname) {
            var name = cname + '=';
            var ca = document.cookie.split(';');
            for(var i = 0; i <ca.length; i++) {

                var c = ca[i];
                while (c.charAt(0)==' ') {
                    c = c.substring(1);
                }
                if (c.indexOf(name) == 0) {
                    return c.substring(name.length,c.length);
                }
            }
            return '';
        } 

        function deleteCookie(cname) {
            document.cookie = cname + '=;expires=Thu, 01 Jan 1970 00:00:01 GMT;';
        }
    </script>
